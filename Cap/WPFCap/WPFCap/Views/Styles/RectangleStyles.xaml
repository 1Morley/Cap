<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Rectangle" x:Key="EqualSidesRectangle">
        <Setter Property="Width" Value="{Binding RelativeSource={RelativeSource Self}, Path=ActualHeight}"/>
        <Setter Property="RadiusY" Value="{Binding RelativeSource={RelativeSource Self}, Path=RadiusX}"/>
    </Style>


    <Style TargetType="Rectangle" x:Key="ProfileImage" BasedOn="{StaticResource EqualSidesRectangle}">
        <Setter Property="RadiusX" Value="{StaticResource ProfileRounding}"/>
        <Setter Property="Height" Value="{StaticResource ProfileSize}"/>
    </Style>
        
    
</ResourceDictionary>