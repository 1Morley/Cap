<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controller="clr-namespace:WPFCap.Controllers"
                    xmlns:select="clr-namespace:WPFCap.Views.DataTemplateSelectors"

>


    <select:ListEntryTypeSelect x:Key="ListEntryTypeSelect"
                                NormalEntryTemplate="{StaticResource ListNormalEntryTemplate}"
                                LocalFileTemplate="{StaticResource ListLocalFileEntryTemplate}"/>








    <ObjectDataProvider x:Key="WindowController"
                        ObjectType="{x:Type controller:WindowController}"
                        MethodName="get_Instance"/>



</ResourceDictionary>